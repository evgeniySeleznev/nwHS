{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "rgba(255, 96, 96, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "legend": {
          "calcs": ["p95"],
          "displayMode": "table",
          "placement": "right"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(holo_request_latency_seconds_bucket{service=\"customer-service\"}[5m])) by (le, endpoint))",
          "legendFormat": "{{endpoint}}"
        }
      ],
      "title": "gRPC latency p95",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "targets": [
        {
          "expr": "sum(rate(holo_requests_total{service=\"customer-service\",status!=\"OK\"}[5m])) / sum(rate(holo_requests_total{service=\"customer-service\"}[5m])) * 100",
          "legendFormat": "error rate"
        }
      ],
      "title": "Error rate",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "targets": [
        {
          "expr": "max(pg_stat_activity_count{service=\"customer-service\"}) / max(pg_settings_max_connections{service=\"customer-service\"}) * 100",
          "legendFormat": "connection usage"
        }
      ],
      "title": "PostgreSQL pool saturation",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "id": 4,
      "targets": [
        {
          "expr": "max(kafka_consumer_lag{group=\"customer-service\"})",
          "legendFormat": "partition lag"
        }
      ],
      "title": "Kafka consumer lag",
      "type": "timeseries"
    }
  ],
  "schemaVersion": 38,
  "tags": ["customer-service", "observability"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timezone": "browser",
  "title": "Customer Service Overview",
  "version": 1
}

